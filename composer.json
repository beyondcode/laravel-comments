{
    "name": "beyondcode/laravel-comments",
    "description": "Add comments to your Laravel application",
    "keywords": [
        "beyondcode",
        "comments",
        "laravel-comments"
    ],
    "homepage": "https://github.com/beyondcode/laravel-comments",
    "license": "MIT",
    "authors": [
        {
            "name": "Marcel Pociot",
            "email": "marcel@beyondco.de",
            "homepage": "https://beyondcode.de",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^3.6|^5.0|^6.0|^7.0|^8.0",
        "phpunit/phpunit": "^9.5.8|^10.0.7"
    },
    "autoload": {
        "psr-4": {
            "BeyondCode\\Comments\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BeyondCode\\Comments\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "BeyondCode\\Comments\\CommentsServiceProvider"
            ]
        }
    }
}
